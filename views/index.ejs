   <% include partials/header.ejs %>

<div class="container">
    <header class="jumbotron">
        <div class="container">
        <h2>Here are the example food images</h2>
        <p><a class="btn btn-primary btn-large" href="/predict">check for a new image</a></p>
        </div>
</header>
<div class="row" style="display:flex; flex-wrap:wrap;">
<% food.forEach(function(foods){ %>
<div class="col-md-3 col-sm-6">
    <div class="thumbnail">
    <img src="<%= foods.image %>">
    <div class="caption">
           <p>
               <a href="/foods/<%= foods._id %>" class="btn btn-primary">More info</a>
           </p>
    </div>
    </div>
</div>
<% }); %>
</div>
</div>

<% include partials/footer.ejs %>
